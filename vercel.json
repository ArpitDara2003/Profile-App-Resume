{
    "version": 2,
    "builds": [
      {
        "src": "profileApp/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "profileApp/wsgi.py" }
    ]
  }
  